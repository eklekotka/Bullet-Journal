<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane fx:id="taskShow" prefHeight="389.0" prefWidth="438.0">
      <children>
         <HBox layoutX="1.0" prefHeight="40.0" prefWidth="434.0">
            <children>
               <Button fx:id="taskDelete" alignment="BASELINE_CENTER" mnemonicParsing="false" prefHeight="64.0" prefWidth="106.0" text="Delete" />
               <Label fx:id="taskNameShow" alignment="CENTER" prefHeight="40.0" prefWidth="433.0" text="name">
                  <font>
                     <Font size="24.0" />
                  </font>
               </Label>
            </children>
         </HBox>
         <HBox layoutX="2.0" layoutY="252.0" prefHeight="70.0" prefWidth="438.0">
            <children>
               <CheckBox fx:id="complete" mnemonicParsing="false" prefHeight="73.0" prefWidth="208.0" text="Complete?">
                  <font>
                     <Font size="18.0" />
                  </font>
               </CheckBox>
            </children>
         </HBox>
         <HBox layoutX="-2.0" layoutY="322.0" prefHeight="66.0" prefWidth="445.0">
            <children>
               <Label text="                                           " />
               <Button fx:id="taskExit" mnemonicParsing="false" prefHeight="58.0" prefWidth="146.0" text="Exit" />
            </children>
         </HBox>
         <Label fx:id="taskDescriptionShow" layoutX="2.0" layoutY="81.0" prefHeight="182.0" prefWidth="446.0">
            <font>
               <Font size="18.0" />
            </font>
         </Label>
      </children></AnchorPane>
</Scene>
